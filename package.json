    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "busboy": "^0.3.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "ddos": "^0.2.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validation": "^1.0.2",
    "firebase-admin": "^8.12.1",
    "helmet": "^3.18.0",
    "http-status": "^1.3.2",
    "joi": "^14.3.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.14",
    "luxon": "^1.17.1",
    "moment-timezone": "^0.5.26",
